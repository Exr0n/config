set smartindent
set shiftwidth=4

inoremap jt <Esc>:w<Return>

command IDE source $VIMCONFIG/idemode.vim

let idemode=$IDEMODE
if idemode == '1'
    IDE
else
    source $VIMCONFIG/autoload/plug.vim
    call plug#begin($VIMCONFIG.'/plugged')
    Plug 'christoomey/vim-tmux-navigator'
    call plug#end()
endif
